{
  "name": "fe-pipeline",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=development ts-node ./server/src/main.ts",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:debug:proxy": "cross-env NODE_ENV=development PROXY_BACKEND_HOST=127.0.0.1 PROXY_BACKEND_PORT=80 nest start --debug --watch",
    "start:prod": "cd ui && yarn run build:prod && cd ../ && yarn run build && rm -rf ./fe-pipeline-home/public && mkdir -p ./fe-pipeline-home && cp -r ./ui/dist ./fe-pipeline-home/public && export NODE_ENV=production && node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.12",
    "@nestjs/core": "^7.6.2",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.12",
    "@nestjs/platform-socket.io": "^7.6.12",
    "@nestjs/platform-ws": "^7.6.12",
    "@nestjs/serve-static": "^1.0.1",
    "@nestjs/swagger": "^4.7.6",
    "@nestjs/typeorm": "^6.2.0",
    "@nestjs/websockets": "^7.6.12",
    "@prisma/client": "^2.22.1",
    "aws-sdk": "^2.909.0",
    "cache-manager": "^3.4.3",
    "cache-manager-redis-store": "^2.0.0",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "extract-zip": "^2.0.1",
    "fs-extra": "^8.1.0",
    "http-proxy-middleware": "^1.0.6",
    "kubernetes-client": "^9.0.0",
    "lodash": "^4.17.20",
    "minio": "^7.0.18",
    "moment": "^2.29.1",
    "mysql": "^2.17.1",
    "nestjs-prisma": "^0.11.0",
    "nestjs-s3": "^1.0.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.6.3",
    "simple-git": "^1.126.0",
    "sqlite3": "^5.0.2",
    "swagger-ui-express": "^4.1.5",
    "typeorm": "^0.2.20",
    "ws": "^7.4.3",
    "yaml": "^1.7.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.6",
    "@nestjs/schematics": "^7.2.0",
    "@nestjs/testing": "^7.6.1",
    "@types/cache-manager": "^3.4.0",
    "@types/express": "^4.17.9",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.165",
    "@types/node": "^12.7.5",
    "@types/node-fetch": "^2.5.8",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.8",
    "@types/yaml": "^1.2.0",
    "cross-env": "^7.0.3",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "prisma": "^2.22.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^4.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
