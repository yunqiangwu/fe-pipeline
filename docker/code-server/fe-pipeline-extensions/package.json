{
	"name": "fe-pipeline-extensions",
	"publisher": "fe-pipeline",
	"enableProposedApi": true,
	"repository": "https://code.choerodon.com.cn/hzero-hzero-c7n-hips/fe-pipeline.git",
	"displayName": "fe-pipeline-extensions",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:fe-pipeline-extensions.helloWorld",
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "fe-pipeline-extensions.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"pkg-vsce": "vsce package",
		"debug-code-server": "code-server --host=0.0.0.0 --extensions-dir `pwd`/../ --port=8080 --auth=none `pwd`/test-project",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.53.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vsce": "^1.87.0",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"socket.io": "^4.0.1"
	}
}
